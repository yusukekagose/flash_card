<table class="table table-hover">
<thead>
  <tr>
    <th>#</th>
    <th>Name</th>
    <th>Email</th>
    <th>Role</th>
    <th>編集</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
  <tr>
    <td scope="row"><%= user.id %></td>
    <td><%= user.nick_name %></td>
    <td><%= user.email %></td>
    <td><%= user.roles[0] %></td>
    <td><%= link_to cancel_user_registration_path(user.id) %></td>
  </tr>
   <% end %>
</tbody>
</table>