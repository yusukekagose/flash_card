<div class="container">
<form>
<%=  simple_form_for(object) do |f| %>
  <div class="form-group">
    <%= f.input :sentence, label: "英文を入力してください",html: { class: 'form-control' } %>
  </div>

  <div class="form-group">
    <%= f.input :translation,label: "翻訳した日本語を入力してください", html: { class: 'form-control' } %>
  </div>

  <div class="form-group">
    <%= f.input :meaning_id,input_html: { value: "#{@meaning}" } %>
  </div>

  <div class="form-group">
    <%= f.submit "送信", class: "btn btn-primary"  %>
  </div>
<% end %>
</form>
</div>
